---
import type { Props } from '@astrojs/starlight/props'
import Default from '@astrojs/starlight/components/ContentPanel.astro'
const pageSlug = Astro.props.slug
---
{
  pageSlug === 'openapi/rapidoc/pokeapi' || pageSlug === 'openapi/stoplight-elements/pokeapi' ? (
    <section>
      <slot />
    </section>
  ) : (
    <Default {...Astro.props}>
      <slot />
    </Default>
  )
}

<style>
  section:first-child {
    display: none;
  }
</style>
